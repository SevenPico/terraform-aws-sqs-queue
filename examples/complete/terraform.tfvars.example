# Copy this file to terraform.tfvars and customize the values below

# Context Configuration
namespace   = "sp"
environment = "dev"
stage       = "test"
name        = "sqs-example"

# Additional tags
tags = {
  "Owner"       = "platform-team"
  "Environment" = "development"
  "Project"     = "sqs-queue-module"
  "Repository"  = "terraform-aws-sqs-queue"
}

# Optional: Override default label order
# label_order = ["namespace", "environment", "stage", "name"]

# Optional: Set custom delimiter
# delimiter = "-"

# Optional: Add attributes
# attributes = ["v1"]
